  "keywords": [
    "Express",
    "MongoDB",
    "React",
    "Redux",
    "Token Authentication",
    "Airbnb Eslint",
    "SCSS",
    "Babel",
    "Webpack Configuration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimitriMikadze/node-redux-auth"
  },
  "author": "Dimtiri Mikadze",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bitcoin-core": "^3.0.0",
